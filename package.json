{
  "name": "local-ralph",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "fabrik": "dist/fabrik"
  },
  "scripts": {
    "build": "bun run scripts/embed-assets.ts && bun build src/fabrik/bin.ts --compile --outfile dist/fabrik",
    "cli": "bun run src/fabrik/bin.ts",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "changeset-publish": "bun run build && node scripts/publish.mjs"
  },
  "dependencies": {
    "@effect/cli": "latest",
    "@effect/platform-node": "latest",
    "effect": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@changesets/changelog-github": "latest",
    "@changesets/cli": "latest"
  },
  "trustedDependencies": [
    "@parcel/watcher"
  ]
}
